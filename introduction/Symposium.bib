@article{babakSearchingGravitationalWaves2013,
  title = {Searching for Gravitational Waves from Binary Coalescence},
  author = {Babak, S. and Biswas, R. and Brady, P. R. and Brown, D. A. and Cannon, K. and Capano, C. D. and Clayton, J. H. and Cokelaer, T. and Creighton, J. D. E. and Dent, T. and Dietz, A. and Fairhurst, S. and Fotopoulos, N. and Gonzalez, G. and Hanna, C. and Harry, I. W. and Jones, G. and Keppel, D. and McKechan, D. J. A. and Pekowsky, L. and Privitera, S. and Robinson, C. and Rodriguez, A. C. and Sathyaprakash, B. S. and Sengupta, A. S. and Vallisneri, M. and Vaulin, R. and Weinstein, A. J.},

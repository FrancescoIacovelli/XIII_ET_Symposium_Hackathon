}

@article{damourMeasurabilityTidalPolarizability2012,
  title = {Measurability of the Tidal Polarizability of Neutron Stars in Late-Inspiral Gravitational-Wave Signals},
  author = {Damour, Thibault and Nagar, Alessandro and Villain, Loic},
  date = {2012-06-15},
  journaltitle = {Physical Review D},
  shortjournal = {Phys. Rev. D},
  volume = {85},
  number = {12},
  eprint = {1203.4352},
  eprinttype = {arxiv},
  eprintclass = {astro-ph, physics:gr-qc},
  pages = {123007},
  issn = {1550-7998, 1550-2368},
  doi = {10.1103/PhysRevD.85.123007},
  url = {http://arxiv.org/abs/1203.4352},
  urldate = {2022-07-28},
  abstract = {The gravitational wave signal from a binary neutron star inspiral contains information on the nuclear equation of state. This information is contained in a combination of the tidal polarizability parameters of the two neutron stars and is clearest in the late inspiral, just before merger. We use the recently defined tidal extension of the effective one-body formalism to construct a controlled analytical description of the frequency-domain phasing of neutron star inspirals up to merger. Exploiting this analytical description we find that the tidal polarizability parameters of neutron stars can be measured by the advanced LIGO-Virgo detector network from gravitational wave signals having a reasonable signal-to-noise ratio of \$\textbackslash rho=16\$. This measurability result seems to hold for all the nuclear equations of state leading to a maximum mass larger than \$1.97M\_\textbackslash odot\$. We also propose a promising new way of extracting information on the nuclear equation of state from a coherent analysis of an ensemble of gravitational wave observations of separate binary merger events.},
  keywords = {Astrophysics - High Energy Astrophysical Phenomena,General Relativity and Quantum Cosmology},
  file = {/home/jacopo/Zotero/storage/GVCZ36S8/Damour et al. - 2012 - Measurability of the tidal polarizability of neutr.pdf;/home/jacopo/Zotero/storage/8LCHRRZ6/1203.html}
}

@article{dupletsaGwfishSimulationSoftware2023,
  title = {Gwfish: {{A}} Simulation Software to Evaluate Parameter-Estimation Capabilities of Gravitational-Wave Detector Networks},
  shorttitle = {Gwfish},
  author = {Dupletsa, U. and Harms, J. and Banerjee, B. and Branchesi, M. and Goncharov, B. and Maselli, A. and Oliveira, A. C. S. and Ronchini, S. and Tissino, J.},
